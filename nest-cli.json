{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/client/src",
  "monorepo": true,
  "root": "apps/client",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs_microservice/tsconfig.app.json"
  },
  "projects": {
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "socket": {
      "type": "application",
      "root": "apps/socket",
      "entryFile": "main",
      "sourceRoot": "apps/socket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/socket/tsconfig.app.json"
      }
    }
  }
}